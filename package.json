{
  "name": "@mborne/markdown-to-html",
  "version": "0.6.0-pre",
  "description": "Experiment static site renderer/server supporting markdown and HTML/JS views",
  "main": "index.js",
  "bin": {
    "markdown-to-html": "bin/markdown-to-html.js"
  },
  "scripts": {
    "build-docs": "rm -rf docs && markdown-to-html -m convert -l default -O docs sample",
    "test": "npm run check-style",
    "check-style": "prettier --check \"src/**/*.js\" \"bin/*.js\"",
    "fix-style": "prettier --write \"src/**/*.js\"  \"bin/*.js\""
  },
  "author": "Mickael Borne <mborne@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mborne/markdown-to-html.git"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^4.1.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "markdown-toc": "^1.2.0",
    "marked": "^0.8.0",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "prettier": "1.19.1"
  }
}
